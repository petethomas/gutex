{
  "compilerOptions": {
    "target": "ES2020",
    "module": "None",
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "strict": false,
    "skipLibCheck": true,
    "declaration": false,
    "sourceMap": false,
    "removeComments": false,
    "isolatedModules": false,
    "outFile": "./dist/web-ui.js",
    "noImplicitAny": false,
    "noImplicitThis": false,
    "strictNullChecks": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false
  },
  "files": [
    "src/web-ui/web-ui-all.ts"
  ]
}
